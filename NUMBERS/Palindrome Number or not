#include <stdio.h>

int main() {
    int d,n,sum=0,a;
    printf("enter number: ");
    scanf("%d",&n);
    a=n;
    while(n!=0)
    {
        d=n%10;
        sum=sum*10+d;
        n=n/10;
    }
    if(a==sum)
    {
        printf("True");
    }
    else
    {
        printf("false");
    }
    return 0;
}
